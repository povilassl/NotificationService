{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "NotificationServices": {
    "EmailNotificationServices": [
      {
        "Name": "AmazonSnsEmailNotificationService",
        "IsEnabled": false,
        "Priority": 1
      },
      {
        "Name": "TwilioEmailNotificationService",
        "IsEnabled": true,
        "Priority": 2
      },
      {
        "Name": "VonageEmailNotificationService",
        "IsEnabled": true,
        "Priority": 3
      }
    ],
    "SmsNotificationServices": [
      {
        "Name": "AmazonSnsSmsNotificationService",
        "IsEnabled": true,
        "Priority": 1
      },
      {
        "Name": "TwilioSmsNotificationService",
        "IsEnabled": true,
        "Priority": 2
      },
      {
        "Name": "VonageSmsNotificationService",
        "IsEnabled": true,
        "Priority": 3
      }
    ]
  }
}
